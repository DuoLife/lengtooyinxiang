<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper  
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"  
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd"> 
<mapper namespace="LengtooWallpaper">
	<select id="selectAll" parameterType="String" resultType="LengtooWallpaper">
		select * from lengtoo_bizhi where createdate &lt; #{createdate} order by createdate desc limit 20
	</select>
</mapper>
